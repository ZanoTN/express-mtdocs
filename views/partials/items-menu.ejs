<% items.forEach(item => { %>
  <%# Is a folder %>
  <% if (item.children !== undefined) { %>
    <a href="#" class="">
      <%= item.name %>
    </a>
    <div class="d-flex flex-column ms-4">
      <%- include('./items-menu', { items: item.children }) %>
    </div>
  <% } else { %>
    <%# Is a file %>
    <a href="<%= item.url %>" class="">
      <%= item.name %>
    </a>
  <% } %>
<% }); %>